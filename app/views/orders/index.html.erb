<h1>Orders</h1>


<% @orders.each do |order| %>
    <ul>
      <!-- <li>Name: <%#= order.name %></li>
      <li>Address: <%#= order.address1 %></li> -->
      <li>Status: <%= order.status %></li>
      <li>Items: </li>
        <% order.items.each do |item| %>
            <li>Product:
                <%= item.product.product_name %>;
                Quantity:
                <%= item.quantity %>;
            Product Price: <%= item.product.price %>
            <li>Total: <%=item.subtotal %></li>
            </li>
        <% end %>
      </ul>
    </ul>
<% end %>



<button id="order_display">Load Orders</button>
<button id="stop_live_orders">Stop Live Orders</button>


<div class="clear_display">
    <p class="order_display">test</p>
    <div id="order_display"></div>
</div>
